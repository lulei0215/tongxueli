
<!--page/percent/percent.wxml-->
<view class="container">
  <view class="page-body">
    <view class="per_topbg">
      <image src="{{info.img}}" class="img_circle"></image>
      <view class="ft28">{{info.name}}</view>
    </view>
    <view class="per_mokuai fl w100 bgwhite">
      <navigator class="permk_li" url="/page/mycollect/mycollect">
        <icon class="icon_right1 fr"></icon>
        <image src="{{picPath}}/icon_collect.png"></image>
        <text>我的收藏</text>
      </navigator>
      <navigator class="permk_li" url="/page/myscore/myscore">
        <icon class="icon_right1 fr "></icon>
        <image src="{{picPath}}/icon_score.png"></image>
        <text>我的积分</text>
      </navigator>
    </view>
    <view class="position_fixed fl w100">
        <navigator open-type='redirect' url="/page/index/index">
            <icon class="icon_cander"></icon><!--未选中：icon_cander；选中：icon_cadhover-->
            <view>同学历</view>
            <!--当前状态，给view 加类名：ftblue-->
        </navigator>
        <navigator url="">
            <icon class="icon_perhover"></icon><!--未选中：icon_person；选中：icon_perhover-->
            <view class="ftblue">个人中心</view>
        </navigator>
    </view>
  </view>
</view>