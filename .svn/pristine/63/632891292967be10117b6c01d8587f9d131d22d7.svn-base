
<!--page/percent/percent.wxml-->
<view class="container">
  <view class="page-body">
    <view class="marb100 fl w100">
    <view class="per_topbg">
      <image src="{{info.img}}" class="img_circle"></image>
      <view class="">{{info.name}}</view>
    </view>
    <view class="per_mokuai bgwhite">
      <navigator class="permk_li" url="/page/mycollect/mycollect">
        <icon class="icon_right1 fr"></icon>
        <image src="{{picPath}}/icon_collect1.png"></image>
        <text class="ftblod">我的收藏</text>
      </navigator>
      <navigator class="permk_li" url="/page/mycreate/mycreate">
        <icon class="icon_right1 fr"></icon>
        <image src="{{picPath}}/icon_chuang.png"></image>
        <text class="ftblod">我的创作</text>
      </navigator>
      <navigator class="permk_li" url="/page/myscore/myscore">
        <icon class="icon_right1 fr "></icon>
        <image src="{{picPath}}/icon_score.png"></image>
        <text class="ftblod">我的积分</text>
      </navigator>
      <navigator class="permk_li" url="/page/mygift/mygift">
        <icon class="icon_right1 fr "></icon>
        <image src="{{picPath}}/icon_lipin.png"></image>
        <text class="ftblod">礼品兑换</text>
      </navigator>
      <navigator class="permk_li" url="/page/myorder/myorder">
        <icon class="icon_right1 fr "></icon>
        <image src="{{picPath}}/icon_order.png"></image>
        <text class="ftblod">订单中心</text>
      </navigator>
    </view>
    </view>
    <view class="position_fixed fl w100">
      <navigator open-type='reLaunch' url="/page/index/index" class="">
        <icon class="icon_index"></icon>
        <!--未选中：icon_index；选中：icon_indhover-->
        <text>同学历</text>
      </navigator>
      <navigator open-type='reLaunch' url="/page/candindex/candindex">
        <icon class="icon_cander"></icon>
        <!--未选中：icon_cander；选中：icon_cadhover-->
        <text class="">写日历</text>
      </navigator>
      <navigator url="" class="bdnav_hover">
        <icon class="icon_perhover"></icon>
        <!--未选中：icon_person；选中：icon_perhover-->
        <text>个人中心</text>
      </navigator>
    </view>
  </view>
</view>